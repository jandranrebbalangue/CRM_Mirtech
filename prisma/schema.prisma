// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Clients {
    id        Int @id @default(autoincrement())
    createdAt String?
    updatedAt String?
    contact String
    organization String
    name String
    status String
    assignedUser String @default("")
}
